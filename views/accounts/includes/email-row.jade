.row
    .col.s8.input-field
        input(type="email", name="emails[#{index}][value]", id="emails_value_#{index}")
        label(for="emails_value_#{index}") Email
    .col.s4.input-field
        select(id="emails_type_#{index}")
            option Home
            option Work
            option Other
        label(for="emails_type_#{index}") Type
